{
	"name": "roblox-ts-game",
	"version": "1.0.0",
	"description": "",
	"main": "index.js",
	"scripts": {
		"build": "rojo build --output game.rbxlx",
		"chk": "npm run \"chk:eslint\" && npm run \"chk:prettier\" && npm run \"chk:lua\"",
		"chk:eslint": "eslint \"./src/**/*.*\"",
		"chk:prettier": "prettier --check \"./src/**/*.*\"",
		"chk:lua": "make fmtchk",
		"compile": "rbxtsc --verbose",
		"fix": "npm run \"fix:eslint\" && npm run \"fix:prettier\" && npm run \"fix:lua\"",
		"fix:eslint": "eslint --fix \"./src/**/*.*\"",
		"fix:prettier": "prettier --write \"./src/**/*.*\"",
		"fix:lua": "make fmtfix",
		"pretest": "npm run compile && npm run build",
		"postinstall": "make rm-pkg-specs",
		"test": "npm run test:now",
		"test:now": "run-in-roblox --place game.rbxlx --script ci/unit-tests.lua",
		"watch": "rbxtsc --verbose -w"
	},
	"keywords": [],
	"author": "",
	"license": "ISC",
	"devDependencies": {
		"@rbxts/compiler-types": "^1.3.3-types.1",
		"@rbxts/types": "^1.0.593",
		"@typescript-eslint/eslint-plugin": "^5.26.0",
		"@typescript-eslint/parser": "^5.26.0",
		"eslint": "^8.16.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-prettier": "^4.0.0",
		"eslint-plugin-roblox-ts": "^0.0.32",
		"prettier": "^2.6.2",
		"rbxts-transformer-flamework": "^1.0.0-beta.10",
		"rbxts-transformer-t": "^1.1.5",
		"typescript": "^4.7.2"
	},
	"dependencies": {
		"@flamework/core": "^1.0.0-beta.modding.3",
		"@flamework/networking": "^1.0.0-beta.6",
		"@rbxts/altmake": "^0.1.5",
		"@rbxts/beacon": "^1.2.0",
		"@rbxts/bin": "^1.2.6",
		"@rbxts/log": "^0.6.3",
		"@rbxts/matter": "^0.1.2-ts.8",
		"@rbxts/profileservice": "^1.4.1",
		"@rbxts/promise-character": "^1.0.3",
		"@rbxts/roact": "^1.4.2-ts.0",
		"@rbxts/roact-rodux": "^0.2.2-ts.5",
		"@rbxts/rodux": "^3.0.0-ts.3",
		"@rbxts/rust-classes": "^0.11.2",
		"@rbxts/services": "^1.2.0",
		"@rbxts/sift": "^0.0.1",
		"@rbxts/t": "^3.0.0",
		"@rbxts/testez": "^0.3.1-ts.7",
		"@rbxts/variant": "^1.0.1",
		"@rbxts/zone-plus": "^2.2.3"
	}
